import java.util.Scanner;
class Main{
    public static void main (String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int count=0;
        int arr[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        int key=sc.nextInt();
        for(int i=0;i<n;i++)
        {
           if(arr[i]==key)
           count++;
            
        }
        System.out.print("Given no. of occurence is:"+count);
    }
}
