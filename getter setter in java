public class Main{
   private String name;
   private int age;
   public String getname(){
       return name;
   }
   public int getage(){
       return age;
   }
   
   public void setname(String name){
       this.name=name;
   }
   public void setage(int age){
       this.age=age;
   }
   public static void main(String args[])
   {
       Main m=new Main();
       m.setname("dfvgnm");
       m.setage(56);
       System.out.println("name:"+m.getname());
       System.out.print("age:"+m.getage());
       
   }
}
